{
  "name": "core",
  "version": "1.0.0",
  "author": "pleek91",
  "scripts": {
    "dev": "tsc -w & tsc-alias -w & vite build -w",
    "build": "tsc && tsc-alias && vite build",
    "test": "vitest",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "validate:type": "vue-tsc --noEmit"
