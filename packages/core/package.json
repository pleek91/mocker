      "require": "./dist/core.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types/src/index.d.ts",
  "dependencies": {
    "tsc-alias": "^1.8.5",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  }
